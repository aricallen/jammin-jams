    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@sentry/node": "5.12.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "calendar": "^0.1.1",
    "color": "^3.1.2",
    "d3": "^5.15.0",
