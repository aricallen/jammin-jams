{
  "name": "jammin-jams",
  "version": "0.0.1",
  "description": "jam charged with techno",
  "main": "index.js",
  "author": "aricallen",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev",
    "dev": "node --max-old-space-size=8192 node_modules/.bin/webpack-dev-server --mode development --hot",
    "build:local": "TARGET_ENV=local webpack --mode production",
    "build:production": "TARGET_ENV=production webpack --mode production",
    "deploy": "sh scripts/deploy.sh",
    "deploy:local": "yarn clean && yarn build:production && yarn prepare-deploy",
    "deploy:production": "yarn deploy:local && yarn deploy",
    "forever": "forever -a --uid 'jjserver' --killSignal=SIGTERM ./server/index.js",
    "prepare-deploy": "sh scripts/prepare-deploy.sh",
    "run:local": "yarn build:local && yarn prepare-deploy && yarn serve:local",
    "serve:local": "TARGET_ENV=local yarn serve",
    "serve:production": "TARGET_ENV=production yarn serve",
    "serve": "nodemon ./server/index.js",
    "prettify": "pretty-quick",
    "prettify:staged": "pretty-quick --staged",
    "clean": "rm -rf dist/ tmp/ build/",
    "jest": "jest",
    "jest:w": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
